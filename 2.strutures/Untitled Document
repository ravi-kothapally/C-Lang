#include<stdio.h>
struct cat
{
	char name[100];
	char breed[100];
	int age;
}
struct dog
{	
	char name[100];
	char breed[100];
	int age;
}
struct petshop
{
	char owner[100];
	struct cat c[10];
	struct dog d[10];
}
